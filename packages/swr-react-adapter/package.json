{
  "name": "@server-state-manager-adapter/swr-react-adapter",
  "version": "0.1.0",
  "description": "SWR support for React server state management",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist",
    "lint": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "server",
    "state",
    "management",
    "adapter",
    "typescript",
    "react",
    "useQuery",
    "simple"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "effect": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swr": "^2.3.6",
    "vitest": "3.0.5",
    "@testing-library/react": "^14.1.2",
    "@vitest/ui": "^1.2.0",
    "jsdom": "^24.0.0",
    "@vitejs/plugin-react": "^4.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "typescript": ">=4.0.0",
    "swr": "^2.3.6"
  }
}
